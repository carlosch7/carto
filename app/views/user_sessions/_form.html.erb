<%= form_tag user_sessions_path, :method => :post do %>
  <div class="form-4">
   <h1><%= @titulo %></h1>
    <%= label_tag :email %><br/>
    <%= text_field_tag :email,nil, {placeholder: 'Entre Email', class: 'pull-left input-small'} %>
  
    <%= label_tag :password %><br/>
    <%= password_field_tag :password,nil,{placeholder: 'Entre Password', class: 'pull-left input-small'} %>
  
    <%= submit_tag "Login" %>
  </div>
<% end %>